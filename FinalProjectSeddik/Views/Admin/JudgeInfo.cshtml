@{ Layout = null; }
@model ApplicationUser
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>تحديد الوظيفه</title>
    <link rel="stylesheet" href="~/css/styleZ (2).css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <div class="new-container">
        <div class="navigation">
            <ul>
                <li>
                    <a href="#">
                        <span class="icon"></span>
                        <span class="title"></span>
                    </a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="Home">
                        <span class="icon">
                            <i class="bi bi-house-door"></i>
                        </span>
                        <span class="title">الصفحه الرئيسيه</span>

                    </a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="Admin">
                        <span class="icon">
                            <i class="bi bi-house-door"></i>
                        </span>
                        <span class="title">الطلبات و الرسائل</span>

                    </a>
                </li>
                <li>
                    <a asp-action="UserMangment" asp-controller="Admin">
                        <span class="icon"><i class="bi bi-kanban"></i></span>
                        <span class="title">اداره المستخدمين</span>
                    </a>
                </li>

                <li>
                    <a asp-action="Result" asp-controller="Admin">
                        <span class="icon"><i class="bi bi-file-earmark-person"></i></span>
                        <span class="title">النتائج</span>
                    </a>
                </li>
            </ul>
        </div>
        <div class="main">
            <div class="topbar">

                <div class="toggle">
                    <i class="bi bi-list"></i>
                </div>
            </div>
            <div class="userInfo">
                <h1>بيانات المصحح</h1>
                <form>
                    <table>

                        <!--  start head of table -->

                        <tr>

                            <th>الأسم بالكامل</th>
                            <td>@Model.Name</td>

                        </tr>
                        <tr>
                            <th> البريد الالكتروني</th>
                            <td>@Model.Email</td>

                        </tr>
                        <tr>
                            <th>الرقم القومي</th>
                            <td>@Model.NationalID</td>

                        </tr>
                        <tr>
                            <th>التخصص</th>
                            <td>@Model.Specialization</td>

                        </tr>
                     
                        <tr>
                            <th>النوع</th>
                            <td>@Model.gender</td>

                        </tr>
                        <tr>
                            <th>رقم الموبايل</th>
                            <td>@Model.mobileNumber</td>

                        </tr>



                    </table>
                </form>
                <a asp-action="UserMangment" class="btn btn-primary" id="btnrule">رجوع</a>
            </div>
        </div>
    </div>
    <!-- JS codes -->
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <script src="~/js/scriptA.js"></script>
</body>
</html>