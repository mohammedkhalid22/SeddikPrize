@{ Layout = null; }
@model Projects 
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>تحديد الوظيفه</title>
    <link rel="stylesheet" href="~/css/styleC.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <div class="new-container">
    
        <div class="main">
            <div class="topbar">
            </div>
            <div class="contestantrating">
                <h1> تقييم المتسابق</h1>
                <form method="post" asp-controller="Judge" asp-action="CompRate">
                    <table>

                        <!--  start head of table -->

                        <tr>

                            <th>الأسم بالكامل</th>
                            <td>@Model.Name </td>


                        </tr>
                        <tr>
                            <th>لينك المشروع / الدرايف</th>
                            <td><a href="@Model.DriveLink">@Model.DriveLink</a></td>

                        </tr>
                        <tr>
                            <th>التخصص</th>
                            <td>@Model.Specialization</td>
                        </tr>
                       
                        <tr>
                            <th>التقييم</th>
                            <td>
                                <select class="form-select form-select-sm" id="dropdown" aria-label=".form-select-sm example" asp-for="Result">

                                    <option value="امتياز">امتياز</option>
                                    <option value="جيد جدا ">جيد جدا</option>
                                    <option value="جيد ">جيد</option>
                                    <option value="مقبول">مقبول </option>
                                </select>
                            </td>

                        </tr>




                    </table>
                    <button type="submit" class="btn btn-secondary " id="btnrule">حفظ</button>
                    <a class="btn btn-secondary " id="btnrule" href="/Judge/JudgeDeg">
                        رجوع
                    </a>
                </form>
            </div>
        </div>
    </div>
    <!-- JS codes -->
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <script src="../HOME PAGE/script.js"></script>
</body>
</html>